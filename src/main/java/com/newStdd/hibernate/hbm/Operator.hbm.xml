<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.newStdd.po.Operator" table="operator">
        <id name="operatorId" type="java.lang.String">
            <column name="operator_id" length="20" />
            <generator class="assigned" />
        </id>    
        <property name="operatorName" type="java.lang.String">
            <column name="operator_name" length="512" />
        </property>
        <property name="loginCode" type="java.lang.String">
            <column name="login_code" length="50" />
        </property>
        <property name="loginPassword" type="java.lang.String">
            <column name="login_password" length="128" />
        </property>
    </class>
</hibernate-mapping>
